{
    "name": "graham-campbell/logviewer",
    "description": "LogViewer Provides A LogViewer Admin Module For Laravel 4.1/4.2",
    "keywords": ["laravel", "framework", "log viewer", "LogViewer", "logviewer", "LogViewer", "Laravel LogViewer", "Laravel-LogViewer", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/asset"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/filesystem": "~4.1",
        "illuminate/pagination": "~4.1",
        "illuminate/routing": "~4.1",
        "illuminate/session": "~4.1",
        "illuminate/support": "~4.1",
        "illuminate/view": "~4.1",
        "lightgear/asset": "1.4.*",
        "graham-campbell/core": "~1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "suggest": {
        "graham-campbell/navigation": "Easily add a navigation bar."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\LogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\LogViewer\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
